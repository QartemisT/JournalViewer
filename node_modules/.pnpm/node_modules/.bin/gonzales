#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/gonzales.js/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules:/home/runner/work/JournalViewer/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules/gonzales-pe/bin/gonzales.js/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules/gonzales-pe/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules/gonzales-pe/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gonzales-pe/bin/gonzales.js" "$@"
else 
  exec node  "$basedir/../gonzales-pe/bin/gonzales.js" "$@"
fi
