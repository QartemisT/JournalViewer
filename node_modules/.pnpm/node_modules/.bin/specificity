#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/specificity@0.4.1/node_modules/specificity/bin/specificity/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/specificity@0.4.1/node_modules/specificity/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/specificity@0.4.1/node_modules/specificity/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/specificity@0.4.1/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules:/home/runner/work/JournalViewer/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules/specificity/bin/specificity/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules/specificity/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules/specificity/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../specificity/bin/specificity" "$@"
else 
  exec node  "$basedir/../specificity/bin/specificity" "$@"
fi
