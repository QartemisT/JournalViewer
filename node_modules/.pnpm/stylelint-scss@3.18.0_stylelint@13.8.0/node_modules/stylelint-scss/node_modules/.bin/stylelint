#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/stylelint@13.8.0/node_modules/stylelint/bin/stylelint.js/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/stylelint@13.8.0/node_modules/stylelint/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/stylelint@13.8.0/node_modules/stylelint/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/stylelint@13.8.0/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules:/home/runner/work/JournalViewer/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/stylelint-scss@3.18.0_stylelint@13.8.0/node_modules/stylelint-scss/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/stylelint-scss@3.18.0_stylelint@13.8.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@13.8.0/node_modules/stylelint/bin/stylelint.js" "$@"
else 
  exec node  "$basedir/../../../../../stylelint@13.8.0/node_modules/stylelint/bin/stylelint.js" "$@"
fi
