#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/@primer/css@12.7.0/node_modules/@primer/css/bin/primer-migrate/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/@primer/css@12.7.0/node_modules/@primer/css/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/@primer/css@12.7.0/node_modules/@primer/css/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/@primer/css@12.7.0/node_modules/@primer/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/@primer/css@12.7.0/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/@primer/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules:/home/runner/work/JournalViewer/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/@primer/css/bin/primer-migrate/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/@primer/css/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/@primer/css/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/@primer/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@primer/css/bin/primer-migrate" "$@"
else 
  exec node  "$basedir/../@primer/css/bin/primer-migrate" "$@"
fi
