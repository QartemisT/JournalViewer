#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/eslint@7.17.0/node_modules/eslint/bin/eslint.js/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/eslint@7.17.0/node_modules/eslint/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/eslint@7.17.0/node_modules/eslint/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/eslint@7.17.0/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/.pnpm/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules:/home/runner/work/JournalViewer/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/eslint/bin/eslint.js/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/eslint/bin/node_modules:/home/runner/work/JournalViewer/JournalViewer/node_modules/eslint/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../eslint/bin/eslint.js" "$@"
fi
